          RowBox[{"j", "\[Equal]", 
           RowBox[{"i", "+", "1"}]}], "&&", 
          RowBox[{
           RowBox[{"Mod", "[", 
            RowBox[{"j", ",", "2"}], "]"}], "\[Equal]", "0"}]}]}], "\[Rule]", 
        "mu"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
